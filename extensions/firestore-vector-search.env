# Vector Search Extension Configuration
# This extension automatically generates embeddings for notes

# Collection containing documents to embed
# Using collection group to handle all users' notes
COLLECTION_PATH=users/{userId}/notes

# Field containing the text to embed (plain text from note body)
INPUT_FIELD_NAME=body

# Field to store the embedding vector
OUTPUT_FIELD_NAME=embedding

# Embedding model to use (Gemini is free tier)
EMBEDDING_MODEL=gemini

# Distance measure for similarity search
DISTANCE_MEASURE=COSINE

# Update existing documents when extension is installed
DO_BACKFILL=true

# Firebase Functions location
LOCATION=us-central1
