    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@ainexsuite/firebase": "workspace:*",
    "@ainexsuite/theme": "workspace:*",
    "@ainexsuite/types": "workspace:*",
    "@ainexsuite/ui": "workspace:*",
    "chrono-node": "^2.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.344.0",
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
