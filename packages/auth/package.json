{
  "name": "@ainexsuite/auth",
  "version": "1.0.0",
  "description": "SSO authentication with session cookies for AINexSuite",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "default": "./dist/api/index.js"
    },
    "./migrations": {
      "types": "./dist/migrations/index.d.ts",
      "default": "./dist/migrations/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@ainexsuite/firebase": "workspace:*",
    "@ainexsuite/types": "workspace:*",
    "react": "^19.1.0",
    "js-cookie": "^3.0.5",
    "firebase": "^10.7.2",
    "firebase-admin": "^12.0.0",
    "next": "^15.5.6",
    "clsx": "^2.0.0",
    "@stripe/stripe-js": "^5.3.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.11.0",
    "@types/react": "^19.1.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
