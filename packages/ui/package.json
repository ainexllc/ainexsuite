{
  "name": "@ainexsuite/ui",
  "version": "1.0.0",
  "description": "Shared UI components and design system for AINexSuite",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "default": "./dist/components/*.js"
    },
    "./templates": {
      "types": "./dist/templates/index.d.ts",
      "default": "./dist/templates/index.js"
    },
    "./templates/*": {
      "types": "./dist/templates/*.d.ts",
      "default": "./dist/templates/*.js"
    },
    "./styles": "./src/styles/globals.css",
    "./styles/*": "./src/styles/*.css",
    "./showcase": {
      "types": "./dist/showcase/index.d.ts",
      "default": "./dist/showcase/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@ainexsuite/auth": "workspace:*",
    "@ainexsuite/config": "workspace:*",
    "@ainexsuite/firebase": "workspace:*",
    "@ainexsuite/theme": "workspace:*",
    "@ainexsuite/types": "workspace:*",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-toast": "^1.2.15",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@tiptap/core": "^3.14.0",
    "@tiptap/extension-color": "^3.14.0",
    "@tiptap/extension-link": "^3.14.0",
    "@tiptap/extension-placeholder": "^3.14.0",
    "@tiptap/extension-task-item": "^3.14.0",
    "@tiptap/extension-task-list": "^3.14.0",
    "@tiptap/extension-text-style": "^3.14.0",
    "@tiptap/extension-underline": "^3.14.0",
    "@tiptap/pm": "^3.14.0",
    "@tiptap/react": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "detect-gpu": "^5.0.70",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.344.0",
    "next-themes": "^0.4.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "simplex-noise": "^4.0.1",
    "tailwind-merge": "^2.2.0",
    "three": "^0.182.0",
    "zdog": "^1.1.3"
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@types/zdog": "^1.1.7",
    "next": "15.5.9",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
