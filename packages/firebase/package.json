{
  "name": "@ainexsuite/firebase",
  "version": "1.0.0",
  "description": "Firebase integration for AINexSuite",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./admin": {
      "types": "./dist/admin/index.d.ts",
      "default": "./dist/admin/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@ainexsuite/types": "workspace:*",
    "firebase": "^10.7.2",
    "firebase-admin": "^12.0.0"
  },
  "devDependencies": {
    "@ainexsuite/config": "workspace:*",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  }
}
