{
  "name": "@ainexsuite/main",
  "version": "1.0.0",
  "private": true,
  "description": "AINexSuite main dashboard at www.ainexsuite.com",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0 --cache",
