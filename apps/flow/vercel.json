{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd ../.. && pnpm turbo run build --filter=@ainexsuite/flow --force",
  "installCommand": "cd ../.. && rm -rf node_modules .pnpm-store && pnpm install --force",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "ignoreCommand": "cd ../.. && git diff --quiet HEAD^ HEAD -- ./apps/flow ./packages ./package.json ./pnpm-lock.yaml ./turbo.json ./.npmrc"
}
