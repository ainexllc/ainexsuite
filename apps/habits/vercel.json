{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd ../.. && pnpm turbo run build --filter=@ainexsuite/habits --force",
  "devCommand": "pnpm dev",
  "installCommand": "cd ../.. && pnpm install",
